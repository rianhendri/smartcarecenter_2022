<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:app="http://schemas.android.com/apk/res-auto"
	android:layout_width="match_parent"
	android:layout_height="wrap_content"
	android:orientation="vertical" >

	<LinearLayout
		android:layout_width="match_parent"
		android:layout_height="wrap_content"
		android:layout_margin="8dp"
		android:background="@drawable/bgwhitecorner"
		android:orientation="vertical"
		android:padding="8dp"
		android:visibility="gone" >

		<LinearLayout
			android:layout_width="match_parent"
			android:layout_height="wrap_content"
			android:layout_marginTop="8dp"
			android:orientation="horizontal" >

			<LinearLayout
				android:layout_width="0dp"
				android:layout_height="wrap_content"
				android:layout_weight="1"
				android:orientation="vertical" >

				<TextView
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:fontFamily="@font/segoeui"
					android:text="Status:"
					android:textSize="13sp" />

				<TextView
					android:id="@+id/statusservice"
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:fontFamily="@font/seguisb"
					android:text="Asign"
					android:textColor="@color/colorPrimaryDark"
					android:textSize="12sp" />
				>
			</LinearLayout>

			<LinearLayout
				android:layout_width="0dp"
				android:layout_height="wrap_content"
				android:layout_weight="1"
				android:orientation="vertical" >

				<TextView
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:fontFamily="@font/segoeui"
					android:text="Assigned:"
					android:textSize="13sp" />

				<TextView
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:fontFamily="@font/seguisb"
					android:text="2020-08-10T12:38:37.027"
					android:textColor="@color/colorPrimaryDark"
					android:textSize="12sp" />
			</LinearLayout>
		</LinearLayout>

		<LinearLayout
			android:layout_width="match_parent"
			android:layout_height="wrap_content"
			android:layout_marginTop="8dp"
			android:orientation="horizontal" >

			<LinearLayout
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:layout_weight="1"
				android:orientation="vertical" >

				<TextView
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:fontFamily="@font/segoeui"
					android:text="Engineer:"
					android:textSize="13sp" />

				<TextView
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:fontFamily="@font/seguisb"
					android:text="Andre"
					android:textColor="@color/colorPrimaryDark"
					android:textSize="12sp" />
				>
			</LinearLayout>

			<LinearLayout
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:layout_weight="1"
				android:orientation="vertical" >

				<TextView
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:fontFamily="@font/segoeui"
					android:text="Service Type:"
					android:textSize="13sp" />

				<TextView
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:fontFamily="@font/seguisb"
					android:text="Remote"
					android:textColor="@color/colorPrimaryDark"
					android:textSize="12sp" />
				>
			</LinearLayout>
		</LinearLayout>

		<LinearLayout
			android:layout_width="match_parent"
			android:layout_height="wrap_content"
			android:layout_marginTop="8dp"
			android:orientation="horizontal" >

			<LinearLayout
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:layout_weight="1"
				android:orientation="vertical" >

				<TextView
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:fontFamily="@font/segoeui"
					android:text="Unit Category:"
					android:textSize="13sp" />

				<TextView
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:fontFamily="@font/seguisb"
					android:text="Engine"
					android:textColor="@color/colorPrimaryDark"
					android:textSize="12sp" />
				>
			</LinearLayout>

			<LinearLayout
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:layout_weight="1"
				android:orientation="vertical" >

				<TextView
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:fontFamily="@font/segoeui"
					android:text="Issue Category:"
					android:textSize="13sp" />

				<TextView
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:fontFamily="@font/seguisb"
					android:text="Electronics"
					android:textColor="@color/colorPrimaryDark"
					android:textSize="12sp" />
				>
			</LinearLayout>
		</LinearLayout>

		<LinearLayout
			android:layout_width="match_parent"
			android:layout_height="wrap_content"
			android:layout_marginTop="8dp"
			android:orientation="horizontal" >

			<LinearLayout
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:layout_weight="1"
				android:orientation="vertical" >

				<TextView
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:fontFamily="@font/segoeui"
					android:text="Support:"
					android:textSize="13sp" />

				<TextView
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:fontFamily="@font/seguisb"
					android:text="2020-08-10 - 2020-08-10"
					android:textColor="@color/colorPrimaryDark"
					android:textSize="12sp" />
				>
			</LinearLayout>
		</LinearLayout>
	</LinearLayout>

	<LinearLayout
		android:layout_width="match_parent"
		android:layout_height="wrap_content"
		android:layout_margin="8dp"
		android:background="@drawable/bgwhitecorner"
		android:orientation="vertical" >

		<LinearLayout
			android:layout_width="match_parent"
			android:layout_height="wrap_content"
			android:layout_marginTop="12dp"
			android:orientation="vertical"
			android:paddingLeft="12dp"
			android:paddingRight="12dp" >

			<LinearLayout
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:orientation="horizontal" >

				<TextView
					android:id="@+id/statustick"
					android:layout_width="40dp"
					android:layout_height="wrap_content"
					android:fontFamily="@font/segoeuib"
					android:gravity="start"
					android:text=""
					android:textColor="@color/black"
					android:textSize="17sp" />

				<TextView
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:layout_marginRight="40dp"
					android:fontFamily="@font/segoeuib"
					android:gravity="center"
					android:text="Support"
					android:textColor="@color/black"
					android:textSize="17sp" />
			</LinearLayout>

			<ImageView
				android:id="@+id/posbar"
				android:layout_width="match_parent"
				android:layout_height="45dp"
				android:src="@drawable/asign" />

			<LinearLayout
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:orientation="horizontal" >

				<LinearLayout
					android:layout_width="0dp"
					android:layout_height="match_parent"
					android:layout_weight="1"
					android:gravity="center_vertical"
					android:orientation="horizontal" >


					<TextView
						android:id="@+id/bar1"
						android:layout_width="match_parent"
						android:layout_height="wrap_content"
						android:layout_marginLeft="4dp"
						android:fontFamily="@font/segoeui"
						android:gravity="start"
						android:text="Assign"
						android:textColor="@color/black"
						android:textSize="12sp" />
				</LinearLayout>

				<LinearLayout
					android:layout_width="0dp"
					android:layout_height="match_parent"
					android:layout_weight="1"
					android:gravity="center_vertical"
					android:orientation="horizontal" >
					<TextView
						android:id="@+id/bar2"
						android:layout_width="match_parent"
						android:layout_height="wrap_content"
						android:layout_marginLeft="4dp"
						android:fontFamily="@font/segoeui"
						android:gravity="center"
						android:text="On Progress"
						android:textColor="@color/softgrey"
						android:textSize="12sp" />
				</LinearLayout>

				<LinearLayout
					android:layout_width="0dp"
					android:layout_height="wrap_content"
					android:layout_weight="1"
					android:gravity="center_vertical"
					android:orientation="horizontal" >


					<TextView
						android:id="@+id/bar3"
						android:layout_width="match_parent"
						android:layout_height="wrap_content"
						android:layout_marginLeft="4dp"
						android:fontFamily="@font/segoeui"
						android:gravity="center"
						android:text="Waiting For Confirmation"
						android:textColor="@color/softgrey"
						android:textSize="12sp" />
				</LinearLayout>

				<LinearLayout
					android:layout_width="0dp"
					android:layout_height="match_parent"
					android:layout_weight="1"
					android:gravity="center"
					android:orientation="horizontal" >

					<TextView
						android:id="@+id/bar4"
						android:layout_width="match_parent"
						android:layout_height="wrap_content"
						android:layout_marginLeft="4dp"
						android:fontFamily="@font/segoeui"
						android:gravity="end"
						android:text="Solved"
						android:textColor="@color/softgrey"
						android:textSize="12sp" />
				</LinearLayout>
			</LinearLayout>
		</LinearLayout>

		<LinearLayout
			android:layout_width="match_parent"
			android:layout_height="wrap_content"
			android:layout_marginTop="12dp"
			android:orientation="horizontal"
			android:paddingLeft="12dp"
			android:paddingRight="12dp" >

			<LinearLayout
				android:layout_width="0dp"
				android:layout_height="wrap_content"
				android:layout_weight="1"
				android:gravity="bottom"
				android:orientation="vertical" >

				<LinearLayout
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:gravity="start"
					android:orientation="vertical" >

					<TextView
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:fontFamily="@font/segoeui"
						android:text="Support start: "
						android:textColor="@color/black"
						android:textSize="11sp" />

					<TextView
						android:id="@+id/starttime"
						android:layout_width="wrap_content"
						android:layout_height="match_parent"
						android:fontFamily="@font/seguisb"
						android:text="Engine"
						android:textColor="@color/colorPrimaryDark"
						android:textSize="11sp" />
				</LinearLayout>

				<LinearLayout
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:gravity="start"
					android:orientation="vertical" >

					<TextView
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:fontFamily="@font/segoeui"
						android:text="Support Finish: "
						android:textColor="@color/black"
						android:textSize="11sp" />

					<TextView
						android:id="@+id/endtime"
						android:layout_width="wrap_content"
						android:layout_height="match_parent"
						android:fontFamily="@font/seguisb"
						android:text="Electronics"
						android:textColor="@color/colorPrimaryDark"
						android:textSize="11sp" />
				</LinearLayout>
			</LinearLayout>

			<LinearLayout
				android:layout_width="0dp"
				android:layout_height="wrap_content"
				android:layout_weight="1"
				android:orientation="vertical" >

				<LinearLayout
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:gravity="end"
					android:orientation="vertical" >

					<TextView
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:fontFamily="@font/segoeui"
						android:text="Engineer: "
						android:textColor="@color/black"
						android:textSize="11sp" />

					<TextView
						android:id="@+id/engineer"
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:fontFamily="@font/seguisb"
						android:text="Andre"
						android:textColor="@color/colorPrimaryDark"
						android:textSize="11sp" />
				</LinearLayout>

				<LinearLayout
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:gravity="end"
					android:orientation="vertical" >

					<TextView
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:fontFamily="@font/segoeui"
						android:text="Assigned: "
						android:textColor="@color/black"
						android:textSize="11sp" />

					<TextView
						android:id="@+id/assigndate"
						android:layout_width="wrap_content"
						android:layout_height="match_parent"
						android:fontFamily="@font/seguisb"
						android:text="Remote"
						android:textColor="@color/colorPrimaryDark"
						android:textSize="11sp" />
				</LinearLayout>
			</LinearLayout>
		</LinearLayout>

		<LinearLayout
			android:layout_width="match_parent"
			android:layout_height="wrap_content"
			android:orientation="horizontal"
			android:paddingLeft="12dp"
			android:paddingRight="12dp" >

			<LinearLayout
				android:id="@+id/ratingstarlayout"
				android:layout_width="0dp"
				android:layout_height="match_parent"
				android:layout_weight="1"
				android:gravity="bottom"
				android:alpha="0"
				android:orientation="vertical" >

				<LinearLayout
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:layout_marginTop="12dp"
					android:gravity="center"
					android:orientation="horizontal" >

					<androidx.appcompat.widget.AppCompatRatingBar
						android:id="@+id/ratingstar"
						android:layout_width="wrap_content"
						android:layout_height="40dp"
						android:isIndicator="true"
						android:numStars="5"
						android:rating="5"
						android:scaleX=".4"
						android:scaleY=".4"
						android:stepSize="1" />
				</LinearLayout>
				<LinearLayout
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:layout_marginBottom="12dp"
					android:gravity="center"
					android:orientation="horizontal" >

					<TextView
						android:id="@+id/comment"
						android:layout_width="wrap_content"
						android:layout_height="match_parent"
						android:fontFamily="@font/seguisb"
						android:text="''''"
						android:textColor="@color/black"
						android:textSize="11sp" />
				</LinearLayout>
			</LinearLayout>
		</LinearLayout>
	</LinearLayout>

</LinearLayout>
